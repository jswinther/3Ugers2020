%laser "scanpush cmd='zoneobst'"
%startx = $odox
%starty = $odoy

% End condition
%followline "bm" :($odoth < -1.50)
%wait 1
%endx = $odox + $l4
%endy = $odoy
%res = sqrt(endx*endx+endy*endy)
%stringcat "Distance from start to the box is = " res

followline "bl" :($crossingblackline==1)
wait 1
ignoreobstacles
followline "bm" :($crossingblackline==1)
wait 1
followline "bm" :($drivendist==0.1175)
fwd -0.2
followline "bm" :($crossingblackline==1)

